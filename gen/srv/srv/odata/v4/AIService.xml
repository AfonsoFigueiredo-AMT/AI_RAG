<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
    <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:DataServices>
    <Schema Namespace="AIService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <Annotation Term="Core.Links">
        <Collection>
          <Record>
            <PropertyValue Property="rel" String="author"/>
            <PropertyValue Property="href" String="https://cap.cloud.sap"/>
          </Record>
        </Collection>
      </Annotation>
      <EntityContainer Name="EntityContainer">
        <ActionImport Name="queryLLM" Action="AIService.queryLLM"/>
        <ActionImport Name="populateCoordinates" Action="AIService.populateCoordinates"/>
      </EntityContainer>
      <ComplexType Name="return_AIService_queryLLM"/>
      <ComplexType Name="return_AIService_populateCoordinates">
        <Property Name="message" Type="Edm.String"/>
        <Property Name="updated" Type="Collection(AIService.return_AIService_populateCoordinates_updated)" Nullable="true"/>
        <Property Name="failed" Type="Collection(AIService.return_AIService_populateCoordinates_failed)" Nullable="true"/>
      </ComplexType>
      <ComplexType Name="return_AIService_populateCoordinates_updated">
        <Property Name="id" Type="Edm.Int32"/>
        <Property Name="address" Type="Edm.String"/>
        <Property Name="lat" Type="Edm.Double"/>
        <Property Name="lon" Type="Edm.Double"/>
      </ComplexType>
      <ComplexType Name="return_AIService_populateCoordinates_failed">
        <Property Name="id" Type="Edm.Int32"/>
        <Property Name="address" Type="Edm.String"/>
      </ComplexType>
      <Action Name="queryLLM" IsBound="false">
        <Parameter Name="prompt" Type="Edm.String"/>
        <ReturnType Type="AIService.return_AIService_queryLLM"/>
      </Action>
      <Action Name="populateCoordinates" IsBound="false">
        <ReturnType Type="AIService.return_AIService_populateCoordinates"/>
      </Action>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>